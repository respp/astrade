{
  "expo": {
    "name": "AsTrade",
    "slug": "astrade-dapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "astrade",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.astrade.app",
      "usesAppleSignIn": true,
      "associatedDomains": ["applinks:astrade.app"],
                    "config": {
                "googleSignIn": {
                  "reservedClientId": "com.googleusercontent.apps.919801638946-5b67c01ac1aiuq2igm96h1hkus6p2p9u.apps.googleusercontent.com"
                }
              }
    },
    "android": {
      "package": "com.astrade.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0F0F23"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "astrade"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
                    "config": {
                "googleSignIn": {
                  "apiKey": "919801638946-5b67c01ac1aiuq2igm96h1hkus6p2p9u.apps.googleusercontent.com",
                  "certificateHash": "5E:8F:16:06:2E:A3:CD:2C:4A:0D:54:78:76:BA:A6:F3:8F:AB:C6:A4"
                }
              }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
              "expo-web-browser",
        "expo-secure-store"
      ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
