# Cavos Configuration
EXPO_PUBLIC_CAVOS_API_KEY=your-cavos-api-key
EXPO_PUBLIC_CAVOS_HASH_SECRET=your-cavos-hash-secret
EXPO_PUBLIC_CAVOS_APP_ID=your-cavos-app-id
EXPO_PUBLIC_CAVOS_NETWORK=sepolia

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your-supabase-project-url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
EXPO_PUBLIC_SUPABASE_SERVICE_KEY=your-supabase-service-key

# API Configuration
EXPO_PUBLIC_API_BASE_URL=http://localhost:8000/api/v1
EXPO_PUBLIC_MOCK_MODE=false


# Dojo Integration Environment Variables
# Copy these variables to your .env file or add them to your existing environment configuration

# Enable/Disable Dojo integration
EXPO_PUBLIC_DOJO_ENABLED=true

# Katana RPC URL (local development)
EXPO_PUBLIC_DOJO_RPC_URL=http://localhost:5050

# Torii indexer URL (local development)
EXPO_PUBLIC_DOJO_TORII_URL=http://localhost:8080

# Deployed Dojo world address (get this from `sozo migrate` output)
# Example: EXPO_PUBLIC_DOJO_WORLD_ADDRESS=0x1234567890abcdef...
EXPO_PUBLIC_DOJO_WORLD_ADDRESS=

# Dojo namespace (matches your contracts namespace)
EXPO_PUBLIC_DOJO_NAMESPACE=di

# Chain ID (KATANA for local development, or testnet chain ID)
EXPO_PUBLIC_DOJO_CHAIN_ID=0x4b4154414e41

# Optional: Domain configuration for transaction signing
EXPO_PUBLIC_DOJO_DOMAIN_NAME=astrade-dojo
EXPO_PUBLIC_DOJO_DOMAIN_VERSION=1.0
EXPO_PUBLIC_DOJO_DOMAIN_REVISION=1